<?xml version="1.0" encoding="utf-8"?>
<ResearchProjectDefs>
  <ResearchProjectDef>
    <defName>Alloys</defName>
    <label>Alloys</label>
    <description>Allows the production of plasteel in the electric smelter.</description>
    <baseCost>1400</baseCost>
    <techLevel>Industrial</techLevel>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <prerequisites>
      <li>ElectricSmelting</li>
    </prerequisites>
  </ResearchProjectDef>
  <ResearchProjectDef>
    <defName>AdvancedMetalurgy</defName>
    <label>Advanced Metalurgy</label>
    <description>Allows the fabrication of advanced alloys.</description>
    <baseCost>1400</baseCost>
    <techLevel>Spacer</techLevel>
    <prerequisites>
      <li>Alloys</li>
    </prerequisites>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
  </ResearchProjectDef>
  <ResearchProjectDef>
    <defName>Robotics</defName>
    <label>Robotics</label>
    <description>Allows the construction of the Robot Mining Complex.</description>
    <baseCost>3000</baseCost>
    <techLevel>Industrial</techLevel>
    <prerequisites>
      <li>ComponentAssembly</li>
      <li>Alloys</li>
    </prerequisites>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
  </ResearchProjectDef>
  <ResearchProjectDef>
    <defName>Bioscience</defName>
    <label>Bioscience</label>
    <description>Allows the construction of the bioreactor and the biogenerator.</description>
    <baseCost>1800</baseCost>
    <techLevel>Industrial</techLevel>
  </ResearchProjectDef>
</ResearchProjectDefs>
